<%= form_for(@session) do |f| %>
<% if @session.errors.any? %>
<div id="error_explanation">
	<h2><%= pluralize(@session.errors.count, "error") %> prohibited this session from being saved:</h2>

	<ul>
		<% @session.errors.full_messages.each do |msg| %>
		<li>
			<%= msg %>
		</li>
		<% end %>
	</ul>
</div>
<% end %>

<div class="field">
	<%= f.label :begintime %>
	<br />
	<%= f.datetime_select :begintime %>
</div>
<div class="field">
	<%= f.label :endtime %>
	<br />
	<%= f.datetime_select :endtime %>
</div>
<div class="field">
	<%= f.label :notes %>
	<br />
	<%= f.text_area :notes %>
</div>

<div class="field">
	<%= f.label :name, ' 1. Injection Concerns - IM' %>
	<%= f.check_box :b1 %>
	<%= f.collection_select :bd1, Session::BARRIERS1, :to_s, :to_s, :include_blank => true %>
</div>

<div class="field">
	<%= f.label :name,'2. Injection Concerns - SC' %>
	<%= f.check_box :b2 %>
	<%= f.collection_select :bd2, Session::BARRIERS2, :to_s, :to_s, :include_blank => true %>
</div>

<div class="field">
