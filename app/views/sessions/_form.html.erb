	<%= f.collection_select :bd3, Session::BARRIERS3, :to_s, :to_s, :include_blank => true %>
</div>

<div class="field">
	<%= f.label :name,'4. Side Effects ...................' %>
	<%= f.check_box :b4 %>
	<%= f.collection_select :bd4, Session::BARRIERS4, :to_s, :to_s, :include_blank => true %>
