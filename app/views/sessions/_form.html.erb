  </div>
  <div class="field">
    <%= f.label :endtime %><br />
    <%= f.datetime_select :endtime %>
  </div>
  <div class="field">
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </div>
  
  
   <div class="field">
    <%= f.label :'Injection Concerns' %><br />
    <%= f.check_box :b1 %>
  </div>
  
  <div class="field">
    <%= f.label :'Financial Concerns' %><br />
    <%= f.check_box :b2 %>
  </div>
  
  <div class="field">
    <%= f.label :'Side Effects' %><br />
    <%= f.check_box :b3 %>
  </div>
  
  
  <div class="field">
    <%= f.label :'Is it worth it' %><br />
    <%= f.check_box :b4 %>
  </div>
  <div class="field">
    <%= f.label :'Building Routines' %><br />
    <%= f.check_box :b5 %>
  </div>
  <div class="field">
    <%= f.label :'Other Concerns, Emotional Barriers' %><br />
    <%= f.check_box :b6 %>
  </div>
  
  
  <div class="actions">
