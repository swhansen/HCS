<h1>Sessions Administration for Patient</h1>

<table>
  <tr>
    
    <th>Begintime</th>   
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    
    <td><%= session.begintime %></td>
    
    <td><%= session.notes %></td>
    <td><%= link_to 'Details', session %></td>
    <td><%= link_to 'Edit', edit_session_path(session) %></td>
    <td><%= button_to 'Remove', session, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Session', new_session_path %>

